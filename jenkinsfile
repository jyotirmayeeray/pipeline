pipeline {
    agent {
        lebel 'slave-1'
    }

    stages {
        stage('Git') {
            steps{
                git branch: 'master', url: 'https://github.com/jyotirmayeeray/pipeline.git'
            }
        }
        stage('shell') {
            steps{
                sh 'bash shell.sh'
            }
        }
    }
}
